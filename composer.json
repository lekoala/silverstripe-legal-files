{
    "name": "lekoala/silverstripe-legal-files",
    "description": "Manage and validates legal documents SilverStripe",
    "type": "silverstripe-module",
    "keywords": [
        "silverstripe",
        "legal",
        "document",
        "module",
        "cms"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "LeKoala",
            "email": "thomas@lekoala.be"
        }
    ],
    "require": {
        "php": ">=7",
        "silverstripe/framework": "^4.1",
        "silverstripe/crontask": "^2.1"
    },
    "extra": {
        "installer-name": "legal-files"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/recipe-plugin": true,
            "silverstripe/vendor-plugin": true
        }
    },
    "support": {
        "issues": "https://github.com/lekoala/silverstripe-legal-files/issues"
    }
}
